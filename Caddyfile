prefect.{$DOMAIN} {
    basicauth {
        {$CADDY_USERNAME} {$CADDY_PASSWORD_HASH}
    }
    reverse_proxy prefect-server:4200
}

webhook.{$DOMAIN} {
    reverse_proxy flask-webhook:5000
}

dashboard.{$DOMAIN} {
    basicauth {
        {$CADDY_USERNAME} {$CADDY_PASSWORD_HASH}
    }
    reverse_proxy streamlit-dashboard:8501
}


